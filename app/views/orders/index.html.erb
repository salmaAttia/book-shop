
        <div class="row">
                <div class="col-md-10 col-md-offset-1">
                    <div class="Order-list">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h4><%= current_user.username%> Order List : </h4>
                            </div>
                            <div class="panel-body">
                                <div class="table-responsive">
                                    <table class="table table-striped table-bordered table-hover">
                                        <thead>
                                            <tr>
                                                <th>Ref. No.</th>
                                                <th>Date</th>
                                                <th>Details</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% @user_orders.each do |userorder|%>
                                            <tr>
                                                <td>#<%= userorder.id%></td>
                                                <td>01/24/2015 </td>
                                                <td>
                                                     <%= link_to  order_path(userorder), class: 'btn btn-info btn-sm' do %>
                                                    <i class="glyphicon glyphicon-eye-open"></i><% end %>
                                                    <%= link_to  order_path(userorder),:method => :delete, class: 'btn btn-danger btn-sm' do %>
                                                    <i class="glyphicon glyphicon-trash"></i><% end %>
                                                </td>
                                            </tr>
                                            <%end%>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- ORDER LIST END-->
                </div>
            </div>
        